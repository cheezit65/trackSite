<div class="contentStyle">
	<h1>Ohio Rockets Roster</h1>
	
	<table>
		<thead>
			<tr>
				<th>Athlete Number</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Year of Birth</th>
				<th colspan="3"></th>
			</tr>
		</thead>

		<tbody>
			<% @athletes.each do |athlete| %>
			<tr>
				<td><%= athlete.athleteNum %></td>
				<td><%= athlete.firstName %></td>
				<td><%= athlete.lastName %></td>
				<td><%= athlete.birthYear %></td>
				<td><%= link_to 'Show', athlete %></td>
				<td><%= link_to 'Edit', edit_athlete_path(athlete) %></td>
				<td><%= link_to 'Destroy', athlete, method: :delete, data: { confirm: 'Are you sure?' } %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
	<br>

	<%= link_to 'New Athlete', new_athlete_path %>
</div>
