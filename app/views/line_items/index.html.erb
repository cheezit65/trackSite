<div class="contentStyle">
	<h1>2015 Meet Results</h1>
<h1>Individual Results</h1>
<%= puts li = Athlete.find(2	) %>
<h3>
<%= "#{li.id} #{li.firstName} #{li.lastName}" %>
<%= puts li2 = Meet.find(li) %>
<%= "#{li2.event1} #{li2.event2} #{li2.event3}" %>
</h3>
<table>
  <thead>
    <tr>
      <th>Athlete</th>
      <th>Meet</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @line_items.each do |line_item| %>
      <tr>
        <td><%= line_item.athlete_id %></td>
        <td><%= line_item.meet_id %></td>
        <td><%= link_to 'Show', line_item %></td>
        <td><%= link_to 'Edit', edit_line_item_path(line_item) %></td>
        <td><%= link_to 'Destroy', line_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Line item', new_line_item_path %>
</div>
