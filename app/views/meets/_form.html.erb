<%= form_for(@meet) do |f| %>
  <% if @meet.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@meet.errors.count, "error") %> prohibited this meet from being saved:</h2>

      <ul>
      <% @meet.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label 'Athlete Number' %><br>
    <%= f.number_field :athleteNum %>
  </div>
  <div class="field">
    <%= f.label '100 Meter Dash' %><br>
    <%= f.number_field(:event1, in: 1.0..200.0, step: 0.01) %>
  </div>
  <div class="field">
    <%= f.label '200 Meter Dash' %><br>
    <%= f.number_field(:event2, in: 1.0..200.0, step: 0.01) %>
  </div>
  <div class="field">
    <%= f.label '400 Meter Dash' %><br>
    <%= f.number_field(:event3, in: 1.0..200.0, step: 0.01) %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
